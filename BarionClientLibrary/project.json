{
  "version": "1.0.0-*",
  "title": "BarionClient",
  "buildOptions": {
    "outputName": "BarionClient",
    "keyFile": "BarionClientSignKey.snk",
    "optimize": true,
    "xmlDoc": true
  },

  "frameworks": {
    "netstandard1.3": {
      "imports": "dnxcore50",
      "dependencies": {
        "Newtonsoft.Json": "9.0.1",
        "System.Net.Http": "4.0.0",
        "System.Net.Requests": "4.0.10",
        "System.Runtime.InteropServices": "4.1.0"
      }
    },
    "net46": {
      "dependencies": {
        "Newtonsoft.Json": "6.0.4"
      },
      "frameworkAssemblies": {
        "System.Net.Http": "4.0.0.0"
      }
    }
  }
}
